print()
for i in range(10):
    print(i, end=",")
print()
for i in range(16):
    print(i, end=",")
print()
for i in range(5, 10):
    print(i, end=",")
print()
for i in range(5, 20, 2):
    print(i, end=",")
print()
for i in range(50, 90, 3):
    print(i, end=",")
print()
for i in range(10, 0, -1):  # <-- fixed colon
    print(i, end=",")
print()
for i in range(15, 5, -5):
    print(i, end=",")
